#vote-data{ style: "display: none;" }
  = @percentage_neti.to_json

.vote-chart-socionics
  - socionics.each do |s|
    .vote-chart-item
      .vote-button
        -# Are :vote_type and data-id redundant or necessary for graceful degradation?
        = link_to image_tag("upvote.png"), send("vote_socionics_#{votable_name}_path", votable, vote_type: "#{s.type_two_im_raw}"),           id: "vote-#{s.type_two_im_raw}",           method: :post,           remote: true, class: "#{@voted_status}"
      .vote-bar-label
        = s.type_two_im
      .vote-bar
        = content_tag(:div, "", class: "bar-#{s.type_two_im_raw}") #slow, change later.
      .vote-readout
        = @percentage_neti #make this dynamic
        \%
